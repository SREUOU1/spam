if not game:IsLoaded() then
    game.Loaded:Wait()
    end 

    game.StarterGui:SetCore("SendNotification", {
        Title = "SPAM ON";
        Text = "Made by SREUOU"; -- what the text says (ofc)
        Duration = 5;
    })
    wait(1)
    game.StarterGui:SetCore("SendNotification", {
        Title = "STARTING...";
        Text = "https://discord.gg/ABfAz3vVzU"; -- what the text says (ofc)
        Duration = 5;
    })

    
Move1 = ""
Move2 = "Trash???"
Move3 = "Sweep Kick"
Move4 = "Neo Wolf Fang Fist"
Move5 = "Kick Barrage"
Move6 = "Meteor Crash"
Move7 = "Wolf Fang Fist"
Move8 = "TS Molotov"
Move9 = "Trash???"
Move10 = "Anger Rush" 

while wait() do

local plr = game.Players.LocalPlayer
for i, v in pairs(plr.Backpack:GetChildren()) do
if
    v.Name == Move1 or v.Name == Move2 or v.Name == Move3 or v.Name == Move4 or v.Name == Move5 or v.Name == Move6 or v.Name == Move7 or v.Name == Move8 or v.Name == Move9 or v.Name == Move10
then
        v.Parent = game.Workspace.Live[plr.Name]
    v:Activate()
   v:Deactivate()
  wait(0.3)
v.Parent = game.Players.LocalPlayer.Backpack
        end
    end
end
